{
    "name": "saro0h/to-do-list",
    "license": "proprietary",
    "type": "project",
    "require": {
        "php": "^8.1",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "doctrine/annotations": "2.0",
        "doctrine/doctrine-bundle": "2.8.2",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "symfony/asset": "5.4.*",
        "symfony/console": "5.4.*",
        "symfony/dotenv": "5.4.*",
        "symfony/flex": "2.2.4",
        "symfony/form": "5.4.*",
        "symfony/routing": "5.4.*",
        "symfony/framework-bundle": "5.4.*",
        "symfony/monolog-bundle": "^3.8",
        "symfony/proxy-manager-bridge": "5.4.*",
        "symfony/security-bundle": "5.4.*",
        "symfony/twig-bundle": "5.4.*",
        "symfony/validator": "5.4.*",
        "symfony/yaml": "5.4.*",
        "twig/extra-bundle": "^3.5",
        "symfony/runtime": "5.4.*",
        "doctrine/orm": "^2.14"
    },
    "require-dev": {
        "symfony/debug-bundle": "5.4.*",
        "symfony/maker-bundle": "1.44",
        "symfony/stopwatch": "5.4.*",
        "symfony/web-profiler-bundle": "5.4.*",
        "phpunit/phpunit": "^8.5",
        "symfony/browser-kit": "5.4.*",
        "symfony/css-selector": "5.4.*",
        "symfony/phpunit-bridge": "^6.2",
        "rector/rector": "^0.15.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "@symfony-scripts"
        ],
        "post-update-cmd": [
            "@symfony-scripts"
        ]
    },
    "extra": {
        "src-dir": "src/App",
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "web",
        "symfony-tests-dir": "tests",
        "symfony-assets-install": "relative",
        "symfony": {
            "allow-contrib": false,
            "require": "5.4.*",
            "symfony/flex": "^1.0"
        },
        "conflict": {
            "symfony/symfony": "*"
        }
    }
}
